<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.wordboxnew"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="17" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.wordboxnew.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="market.MarketActivity"
            android:label="@string/title_activity_market" >
        </activity>
        <activity
            android:name="wordActivity.WordActivity"
            android:label="@string/title_activity_word" >
        </activity>
        <activity
            android:name="my.MyActivity"
            android:label="@string/title_activity_my" >
        </activity>
        <activity
            android:name="setting.SettingActivity"
            android:label="@string/title_activity_setting" >
        </activity>
        <activity
            android:name="com.example.wordboxnew.BlankActivity"
            android:label="@string/title_activity_blank" >
        </activity>
        <activity
            android:name="wordActivity.TofleWordActivity"
            android:label="@string/title_activity_tofle_word" >
        </activity>
        <activity
            android:name="wordActivity.WordTabbarActivity"
            android:label="@string/title_activity_word_tabbar" >
        </activity>
        <activity
            android:name="market.MarketTabberActivity"
            android:label="@string/title_activity_market_tabber" >
        </activity>
        <activity
            android:name="my.MylistActivity"
            android:label="@string/title_activity_mylist" >
        </activity>
        <activity
            android:name="my.Mypage"
            android:label="@string/title_activity_mypage" >
        </activity>
    </application>

</manifest>